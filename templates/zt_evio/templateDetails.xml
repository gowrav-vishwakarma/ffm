			<option value="Fx.Transitions.Bounce.easeInOut">Bounce.easeInOut</option>
			<option value="Fx.Transitions.Elastic.easeIn">Elastic.easeIn</option>
			<option value="Fx.Transitions.Elastic.easeOut">Elastic.easeOut</option>
			<option value="Fx.Transitions.Elastic.easeInOut">Elastic.easeInOut</option>
		</param>
		<param name="spacer@" type="spacer" />
		<param name="gzip_folder" type="text" size="50" default="zt-assets" label="Optimize Folder" description="Folder where optimized css/js files are stored. (Please note: This folder must be writable)." />
		<param name="gzip_merge" type="radio" default="1" label="Merge CSS and JS" description="Choose to merge multiple CSS and JS into one file.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="gzip_optimize_css" type="radio" default="0" label="Optimize CSS" description="This feature will optimize all the CSS files used in your website.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="css-exclude" type="exlfiles" rows="5" cols="28" default="" label="Optimize Exclude CSS" description="Add the CSS files name you do NOT want to optimize." />
		<param name="gzip_optimize_js" type="radio" default="0" label="Optimize JS" description="This feature will optimize all the JS files used in your website.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="js-exclude" type="exlfiles" rows="5" cols="28" default="" label="Optimize Exclude JS" description="Add the JS files name you do NOT want to optimize." />
		<param name="gzip_optimize_html" type="radio" default="1" label="Optimize HTML" description="This feature will optimize all HTML used in your website.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
